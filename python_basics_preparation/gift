pocket_money = float(input())
sales_money_per_day = float(input())
expenses = float(input())
gift_price = float(input())

day_left = 5

total_saved_money = pocket_money * 5 + sales_money_per_day * 5 - expenses
if total_saved_money >= gift_price:
    print(f"Profit: {total_saved_money:.2f} BGN, the gift has been purchased.")
else:
    money_needed = gift_price - total_saved_money
    print(f"Insufficient money: {money_needed:.2f} BGN.")
